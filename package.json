{
  "name": "sticky-notes",
  "version": "1.0.0",
  "description": "Express Rest API for a note management application",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/app.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "test": "jest test --runInBand",
    "test:watch": "jest test --watch",
    "check:lint": "eslint -c .eslintrc.js src/ --max-warnings 0",
    "check:types": "tsc --noemit -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mat-rogoz/sticky-notes.git"
  },
  "author": "Mateusz R",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mat-rogoz/sticky-notes/issues"
  },
  "homepage": "https://github.com/mat-rogoz/sticky-notes#readme",
  "dependencies": {
    "express": "^4.17.2",
    "reflect-metadata": "^0.1.10",
    "sqlite3": "^5.0.2",
    "typeorm": "0.2.41",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^8.0.29",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.3",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "pino": "^7.7.0",
    "pino-pretty": "^7.5.1",
    "ts-node": "3.3.0",
    "typescript": "4.4.4"
  }
}
